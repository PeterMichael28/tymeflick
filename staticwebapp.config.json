{
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.js",
      "*.css",
      "*.png",
      "*.jpg",
      "*.jpeg",
      "*.gif",
      "*.svg",
      "*.ico",
      "*.woff",
      "*.woff2",
      "*.ttf",
      "*.eot",
      "/images/*",
      "/icons/*",
      "/assets/*",
      "/university/*"
    ]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".ico": "image/x-icon"
  }
}
